{
  "name": "yakumo",
  "description": "Manage complex workspaces with ease",
  "version": "2.0.0-alpha.4",
  "type": "module",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "bin": "lib/cli.js",
  "exports": {
    ".": "./lib/index.js",
    "./cli": "./lib/cli.js",
    "./utils": "./lib/utils.js",
    "./list": "./lib/plugins/list.js",
    "./prepare": "./lib/plugins/prepare.js",
    "./publish": "./lib/plugins/publish.js",
    "./run": "./lib/plugins/run.js",
    "./test": "./lib/plugins/test.js",
    "./upgrade": "./lib/plugins/upgrade.js",
    "./version": "./lib/plugins/version.js",
    "./src/*": "./src/*",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "files": [
    "lib"
  ],
  "author": "Shigma <shigma10826@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shigma/yakumo.git"
  },
  "bugs": {
    "url": "https://github.com/shigma/yakumo/issues"
  },
  "homepage": "https://github.com/shigma/yakumo",
  "keywords": [
    "monorepo",
    "workspace",
    "cli",
    "script"
  ],
  "cordis": {
    "hidden": true,
    "ecosystem": {
      "inject": [
        "yakumo"
      ],
      "pattern": [
        "yakumo-*"
      ]
    },
    "exports": {
      "./list": {},
      "./prepare": {},
      "./publish": {},
      "./run": {},
      "./test": {},
      "./upgrade": {},
      "./version": {}
    }
  },
  "devDependencies": {
    "@types/picomatch": "^3.0.2",
    "@types/prompts": "^2.4.9",
    "@types/which-pm-runs": "^1.0.2",
    "@types/yargs-parser": "^21.0.3"
  },
  "dependencies": {
    "cordis": "^4.0.0-beta.0",
    "cosmokit": "^1.8.0",
    "detect-indent": "^7.0.1",
    "execa": "^9.5.2",
    "get-registry": "^1.2.0",
    "globby": "^14.1.0",
    "kleur": "^4.1.5",
    "ora": "^8.2.0",
    "p-map": "^7.0.3",
    "picomatch": "^4.0.2",
    "prompts": "^2.4.2",
    "semver": "^7.7.1",
    "which-pm-runs": "^1.1.0",
    "yargs-parser": "^21.1.1",
    "yargs-unparser": "^2.0.0"
  }
}
